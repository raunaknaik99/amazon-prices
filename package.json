{
        "name": "amazon_prices",
        "version": "1.0.0",
        "description": "Website which notifies the user whenever the price of an amazon product the user wants falls below an amount which the user sets.",
        "main": "app.js",
        "scripts": {
                "client-install": "npm install --prefix client",
                "start": "node app.js",
                "server": "nodemon app.js",
                "client": "npm run serve --prefix client",
                "dev": "concurrently \"npm run server\" \"npm run client\""
        },
        "author": "JerryMouse",
        "license": "MIT",
        "dependencies": {
                "argon2": "^0.26.2",
                "axios": "^0.19.2",
                "cheerio": "^1.0.0-rc.3",
                "cors": "^2.8.5",
                "express": "^4.18.2",
                "express-validator": "^6.4.0",
                "jsonwebtoken": "^8.5.1",
                "mongoose": "^5.9.9",
                "path": "^0.12.7"
        },
        "devDependencies": {
                "concurrently": "^5.1.0",
                "dotenv": "^8.2.0",
                "nodemon": "^2.0.3"
        }
}
